{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "dom",
      "dom.iterable",
      "es2015",
      "es2016",
      "es2017"
    ],
    "noImplicitAny": false,
    "allowJs": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "sourceMap": true,
    "outDir": "./dist",
    "strict": false ,
    "esModuleInterop": true,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
  }
}
